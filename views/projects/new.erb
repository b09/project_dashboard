<h1> New Project Form</h1>
<form action="/projects" method="post">
  <div class="form-wrapper">

    <div class="form-el">
      <label for="name">Name:</label>
      <input type="text" name="name" id="name"/>
    </div>

    <div class="form=el">
      <label for="budget">Alloted Budget:</label>
      <input type="number" name="budget" id="budget"/>
    </div>

    <div class="form-el">
      <label for="start_date">Select start date:</label>
      <input type="date" name="start_date" id="start_date">
    </div>

    <div class="form-el">
      <label for="description">Project Description:</label>
    <textarea rows="4" cols="50" type="text" name="description">
    </textarea>
  </div>

    <label>
    <select name="type">
      <option value="Front End" name="type" id="type">Front End</option>
      <option value="Back End" name="type" id="type">Back End</option>
      <option value="Other" name="type" id="type">Other</option>
    </select>
  </label>


    <div class="form-el">
      <input type="submit" value="Create Project"/>
    </div>

  </div>
</form>
<form action="/projects" method="GET">
  <button type="submit" value="view">Cancel</button>
</form>
